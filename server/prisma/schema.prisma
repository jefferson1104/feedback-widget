generator client {
  provider = "prisma-client-js"
}

// provider = "sqlite" for local, provider = "postgresql" for production
datasource db {
  provider = "postgresql" 
  url      = env("DATABASE_URL")
}

model Feedback {
  id String @id @default(uuid())

  type String
  comment String
  screenshot String?
  createdAt DateTime @default(now())

  @@map("feedbacks")
}
